\documentclass[b4paper, twoside, landscape, twocolumn]{jarticle}
\usepackage[dvipdfmx]{graphicx} %% dvipdfmxの部分はLaTeXの環境に依存する　TeXworks を使っていれば大丈夫、、、きっと
\usepackage{ifthen}

\usepackage{comment}

%%フォントの設定　ひょっとしたらエラーが出るかも
\renewcommand*\familydefault{\sfdefault} 

\pagestyle{empty}

%% コマンドの定義 特に理由がなければ変更しない
%% ここから
\newcommand{\myinclude}[2]{
  \ifthenelse{#1 < 1}
  {
    \setcounter{page}{#1}
    \ifthenelse{\isodd{\thepage}}
    {
      \begin{flushright}
        \\\includegraphics[width=167mm, height=234mm]{#2}\hspace{5mm}
        \newpage
        \\\includegraphics[width=167mm, height=234mm]{#2}\hspace{5mm}
        \newpage
      \end{flushright}
    }
    {
      \begin{flushleft}
        \\\hspace{5mm}\includegraphics[width=167mm, height=234mm]{#2}
        \newpage
        \\\hspace{5mm}\includegraphics[width=167mm, height=234mm]{#2}
        \newpage
      \end{flushleft}
    }
  }
    {
      \setcounter{page}{#1}
      \ifthenelse{\isodd{\thepage}}
    {
      \setcounter{page}{#1}
      \begin{flushright}
        \thepage\hspace{5mm} \hspace{0mm}\\ %%最後の0mmはおまじない
        \includegraphics[width=167mm, height=234mm]{#2}\hspace{5mm}
        \newpage
        \setcounter{page}{#1}
        \thepage\hspace{5mm} \hspace{0mm}\\
        \includegraphics[width=167mm, height=234mm]{#2}\hspace{5mm}
        \newpage
      \end{flushright}
    }
    {
      %%\begin{minipage}{167mm}
      \begin{flushleft}
        \hspace{5mm}\thepage\\
        \hspace{5mm}\includegraphics[width=167mm, height=234mm]{#2}
        \newpage
      %%\end{minipage}
      %%\hspace{15mm}
      %%\begin{minipage}{167mm}
        \hspace{5mm}\thepage\\
        \hspace{5mm}\includegraphics[width=167mm, height=234mm]{#2}
        \newpage
      \end{flushleft}
    }
  }
}
%% ここまで

